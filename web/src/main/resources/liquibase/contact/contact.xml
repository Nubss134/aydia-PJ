<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="create_table_contact" author="ngocnd">
        <createTable tableName="contact">
            <column name="id" type="int(11)" autoIncrement="true">
                <constraints primaryKey="true" />
            </column>


            <column name="name" type="varchar(200)"/>
            <column name="email" type="varchar(200)"/>
            <column name="tel" type="varchar(20)"/>
            <column name="sub" type="varchar(200)"/>
            <column name="message" type="text"/>
            <column name="is_seen" type="boolean"/>


            <column name="created_time" type="TIMESTAMP"/>
            <column name="is_deleted" type="boolean"/>


        </createTable>

        <modifySql dbms="mysql">
            <append value="ENGINE=INNODB CHARSET=utf8mb4 COLLATE utf8mb4_unicode_520_ci" />
        </modifySql>
    </changeSet>

</databaseChangeLog>